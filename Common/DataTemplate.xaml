<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SportsTeamManagementApp.ViewModels"
                    xmlns:view="clr-namespace:SportsTeamManagementApp.Views">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TeamViewModel}">
        <view:TeamView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ScheduleViewModel}">
        <view:ScheduleView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ScoreboardViewModel}">
        <view:ScoreboardView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfileViewModel}">
        <view:ProfileView/>
    </DataTemplate>

</ResourceDictionary>